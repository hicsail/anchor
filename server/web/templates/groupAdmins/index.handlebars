<div class="container-fluid h-100">
  <div class="row h-100">
    {{>dashboardNav}}
    <div class="col-sm-9 col-md-10">
      <div style="padding: 30px">
        <h3>User Group Admins </h3>
        <hr>        
        <br>
        <p>This table will show the clinicians that have access to your personal health information. You can add one above by searching for one.</p>
        <div class="card">
          <div style="padding: 20px;">
            {{#each data as |group|}}
              <div class="card"> 
                <div style="padding: 20px;" >           
                  {{> groupAdminTable roleName=group.role admins=group.admins }}                  
                </div>
              </div>
              <br>
            {{/each}}               
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<script type="text/javascript" src="/public/scripts/groupAdmin.js"></script>
